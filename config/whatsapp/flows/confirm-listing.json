{
  "version": "7.3",
  "data_api_version": "3.0",
  "routing_model": {
    "LISTING_DETAILS": ["PHOTOS"],
    "PHOTOS": []
  },
  "screens": [
    {
      "id": "LISTING_DETAILS",
      "title": "Listing Details",
      "terminal": false,
      "data": {
        "brand": {"type": "string", "__example__": "Sana Safinaz"},
        "pieces": {"type": "string", "__example__": "3-piece"},
        "size": {"type": "string", "__example__": "M"},
        "condition": {"type": "string", "__example__": "Like new"},
        "price": {"type": "string", "__example__": "95"},
        "chest": {"type": "string", "__example__": "36"},
        "hip": {"type": "string", "__example__": "38"},
        "color": {"type": "string", "__example__": "Maroon"},
        "fabric": {"type": "string", "__example__": "Chiffon"},
        "notes": {"type": "string", "__example__": ""}
      },
      "layout": {
        "type": "SingleColumnLayout",
        "children": [
          {
            "type": "TextHeading",
            "text": "Review your listing"
          },
          {
            "type": "TextInput",
            "name": "brand",
            "label": "Brand / Designer",
            "input-type": "text",
            "required": true,
            "init-value": "${data.brand}",
            "helper-text": "Sana Safinaz, Maria B, Khaadi, etc."
          },
          {
            "type": "Dropdown",
            "name": "pieces",
            "label": "Pieces Included",
            "required": true,
            "init-value": "${data.pieces}",
            "data-source": [
              {"id": "1-piece", "title": "1-piece (Kurta only)"},
              {"id": "2-piece", "title": "2-piece"},
              {"id": "3-piece", "title": "3-piece"},
              {"id": "Lehnga", "title": "Lehnga Set"},
              {"id": "Saree", "title": "Saree"},
              {"id": "Sharara", "title": "Sharara Set"},
              {"id": "Gharara", "title": "Gharara Set"},
              {"id": "Other", "title": "Other"}
            ]
          },
          {
            "type": "Dropdown",
            "name": "size",
            "label": "Size",
            "required": true,
            "init-value": "${data.size}",
            "data-source": [
              {"id": "XS", "title": "XS"},
              {"id": "S", "title": "S"},
              {"id": "M", "title": "M"},
              {"id": "L", "title": "L"},
              {"id": "XL", "title": "XL"},
              {"id": "XXL", "title": "XXL"},
              {"id": "Unstitched", "title": "Unstitched"},
              {"id": "Custom", "title": "Custom Size"}
            ]
          },
          {
            "type": "Dropdown",
            "name": "condition",
            "label": "Condition",
            "required": true,
            "init-value": "${data.condition}",
            "data-source": [
              {"id": "New with tags", "title": "New with tags"},
              {"id": "Like new", "title": "Like new"},
              {"id": "Excellent", "title": "Excellent"},
              {"id": "Good", "title": "Good"},
              {"id": "Fair", "title": "Fair"}
            ]
          },
          {
            "type": "TextInput",
            "name": "price",
            "label": "Asking Price ($)",
            "input-type": "number",
            "required": true,
            "init-value": "${data.price}",
            "helper-text": "Your asking price in USD"
          },
          {
            "type": "TextSubheading",
            "text": "Measurements (inches)"
          },
          {
            "type": "TextInput",
            "name": "chest",
            "label": "Chest",
            "input-type": "number",
            "required": true,
            "init-value": "${data.chest}",
            "helper-text": "Chest measurement in inches"
          },
          {
            "type": "TextInput",
            "name": "hip",
            "label": "Hip",
            "input-type": "number",
            "required": true,
            "init-value": "${data.hip}",
            "helper-text": "Hip measurement in inches"
          },
          {
            "type": "TextSubheading",
            "text": "Additional Details (optional)"
          },
          {
            "type": "TextInput",
            "name": "color",
            "label": "Color",
            "input-type": "text",
            "required": false,
            "init-value": "${data.color}",
            "helper-text": "e.g. Maroon with gold embroidery"
          },
          {
            "type": "TextInput",
            "name": "fabric",
            "label": "Fabric",
            "input-type": "text",
            "required": false,
            "init-value": "${data.fabric}",
            "helper-text": "e.g. Chiffon, Silk, Lawn, Cotton"
          },
          {
            "type": "TextArea",
            "name": "notes",
            "label": "Notes",
            "required": false,
            "init-value": "${data.notes}",
            "helper-text": "Any flaws, alterations, or extras included"
          },
          {
            "type": "Footer",
            "label": "Continue to Photos",
            "on-click-action": {
              "name": "navigate",
              "next": {"type": "screen", "name": "PHOTOS"},
              "payload": {
                "brand": "${form.brand}",
                "pieces": "${form.pieces}",
                "size": "${form.size}",
                "condition": "${form.condition}",
                "price": "${form.price}",
                "chest": "${form.chest}",
                "hip": "${form.hip}",
                "color": "${form.color}",
                "fabric": "${form.fabric}",
                "notes": "${form.notes}"
              }
            }
          }
        ]
      }
    },
    {
      "id": "PHOTOS",
      "title": "Add Photos",
      "terminal": true,
      "success": true,
      "data": {
        "brand": {"type": "string", "__example__": "Sana Safinaz"},
        "pieces": {"type": "string", "__example__": "3-piece"},
        "size": {"type": "string", "__example__": "M"},
        "condition": {"type": "string", "__example__": "Like new"},
        "price": {"type": "string", "__example__": "95"},
        "chest": {"type": "string", "__example__": "36"},
        "hip": {"type": "string", "__example__": "38"},
        "color": {"type": "string", "__example__": "Maroon"},
        "fabric": {"type": "string", "__example__": "Chiffon"},
        "notes": {"type": "string", "__example__": ""}
      },
      "layout": {
        "type": "SingleColumnLayout",
        "children": [
          {
            "type": "TextHeading",
            "text": "Add Photos"
          },
          {
            "type": "TextBody",
            "text": "Upload at least 3 clear photos of your item. Include front, back, and any details or flaws."
          },
          {
            "type": "PhotoPicker",
            "name": "photos",
            "label": "Item Photos",
            "description": "Take new photos or choose from gallery",
            "photo-source": "camera_gallery",
            "min-uploaded-photos": 3,
            "max-uploaded-photos": 10,
            "max-file-size-kb": 10240
          },
          {
            "type": "Footer",
            "label": "Submit Listing",
            "on-click-action": {
              "name": "complete",
              "payload": {
                "brand": "${data.brand}",
                "pieces": "${data.pieces}",
                "size": "${data.size}",
                "condition": "${data.condition}",
                "price": "${data.price}",
                "chest": "${data.chest}",
                "hip": "${data.hip}",
                "color": "${data.color}",
                "fabric": "${data.fabric}",
                "notes": "${data.notes}"
              }
            }
          }
        ]
      }
    }
  ]
}
